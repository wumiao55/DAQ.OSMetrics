<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Description>服务器数据(CPU,内存,磁盘,网络等)数据采集类库</Description>
    <PackageReleaseNotes>支持.net6及以上版本</PackageReleaseNotes>
    <Version>1.1.3</Version>
    <RepositoryUrl>https://github.com/wumiao55/DAQ.OSMetrics</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Diagnostics.PerformanceCounter" Version="6.0.2" />
  </ItemGroup>

</Project>
